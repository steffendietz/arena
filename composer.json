{
    "name": "arena/arena",
    "description": "Arena",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "cycle/entity-behavior": "^1.1",
        "doctrine/collections": "^1.6",
        "spiral/cycle-bridge": "^2.1",
        "spiral/framework": "^3.1",
        "spiral/nyholm-bridge": "^1.3",
        "spiral/roadrunner-bridge": "^2.0",
        "spiral/sapi-bridge": "^1.0",
        "spiral/validator": "^1.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.3",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5",
        "spiral-packages/ignition-bridge": "^1.0",
        "spiral/testing": "^2.1"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('.env.sample', '.env');\"",
            "php app.php encrypt:key -m .env",
            "php app.php configure -vv",
            "rr get-binary"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "spiral/composer-publish-plugin": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
