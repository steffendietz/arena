{
    "name": "arena/arena",
    "description": "Arena",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "cycle/annotated": "^2.0",
        "cycle/migrations": "^1.0",
        "cycle/orm": "^1.0",
        "cycle/proxy-factory": "^1.0",
        "ramsey/uuid": "^4.1",
        "spiral/broadcast": "^2.0",
        "spiral/database": "^2.3",
        "spiral/dotenv-bridge": "^1.0",
        "spiral/framework": "^2.6",
        "spiral/jobs": "^2.0",
        "spiral/migrations": "^2.0",
        "spiral/monolog-bridge": "^1.0",
        "spiral/nyholm-bridge": "^1.0",
        "spiral/prototype": "^1.0",
        "spiral/roadrunner": "^1.4",
        "spiral/scaffolder": "^2.0",
        "spiral/stempler-bridge": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('.env.sample', '.env');\"",
            "php app.php encrypt:key -m .env",
            "php app.php configure -vv"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "publish-cmd": "php app.php publish"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
