{
    "name": "arena/arena",
    "description": "Arena",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*",
        "doctrine/collections": "^1.6",
        "spiral/cycle-bridge": "^1.0",
        "spiral/framework": "^2.9",
        "spiral/nyholm-bridge": "^1.0",
        "spiral/roadrunner-bridge": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spiral/testing": "^1.1"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('.env.sample', '.env');\"",
            "php app.php encrypt:key -m .env",
            "php app.php configure -vv",
            "rr get-binary"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "spiral/composer-publish-plugin": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "/usr/local//share/app-scripts/roadrunner-bridge"
        }
    ]
}
