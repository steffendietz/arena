{
	"name": "spiral",
	"dockerComposeFile":["docker-compose.yml"],
	"service": "app",
	"features": {
		"ghcr.io/devcontainers/features/node:1": {
			"version": "latest"
		}
	},
	"extensions": [
		"xdebug.php-debug",
		"bmewburn.vscode-intelephense-client",
		"mrmlnc.vscode-apache",
		"svelte.svelte-vscode"
	],
	"forwardPorts": [
		8080
	],
	"workspaceFolder": "/workspace"
}